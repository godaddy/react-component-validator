{
  "name": "react-component-validator",
  "version": "2.0.0",
  "description": "Validate Components through propTypes",
  "main": "lib/index.js",
  "browser": "index.js",
  "scripts": {
    "prepublish": "npm run transpile",
    "transpile": "mkdir -p lib && babel -o ./lib/index.js ./index.js",
    "test": "mocha --spec reporter --compilers js:babel-register ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/react-component-validator.git"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-es2015"
    ]
  },
  "author": "GoDaddy.com LLC",
  "license": "MIT",
  "devDependencies": {
    "assume": "~1.4.0",
    "babel": "~6.5.1",
    "babel-cli": "~6.5.1",
    "babel-register": "~6.5.1",
    "istanbul": "~0.4.2",
    "mocha": "~2.4.5",
    "pre-commit": "~1.1.2"
  },
  "dependencies": {
    "babel-preset-es2015": "~6.5.0",
    "babelify": "~7.3.0"
  }
}
